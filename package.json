{
  "name": "dill",
  "description": "Delicious Cucumber tests",
  "keywords": [
    "cucumber",
    "selenium-webdriver",
    "testing"
  ],
  "author": "MojoTech",
  "version": "0.1.0",
  "license": "MIT",
  "engines": {
    "node": ">=0.8.0"
  },
  "directories": {
    "lib": "./lib",
    "bin": "./bin"
  },
  "main": "./lib/dill",
  "bin": {
    "dill": "./bin/dill"
  },
  "scripts": {
    "build": "coffee -o lib/ -c src/",
    "clean": "rm -rf lib",
    "watch": "coffee -o lib/ -cw src/",
    "publish": "npm run-script build && npm publish . && npm run-script clean"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mojotech/dill.js.git"
  },
  "devDependencies": {
    "coffee-script": "1.7.1"
  },
  "dependencies": {
    "cucumber": "~0.3.1",
    "selenium-webdriver": "2.41.0",
    "lodash": "~2.2.1",
    "chai": "~1.8.1",
    "chai-as-promised": "~4.1.0"
  }
}
